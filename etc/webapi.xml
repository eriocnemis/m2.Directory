<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/directory/region/:regionId" method="GET">
        <service class="Eriocnemis\Directory\Api\Region\GetByIdInterface" method="execute"/>
        <resources>
            <resource ref="Eriocnemis_Directory::region"/>
        </resources>
    </route>
</routes>